      "id": "libil2cpp",
      "versionRange": "^0.4.0",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.2.0",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^4000.0.0",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.0",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.0.0",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "tinyxml2",
      "versionRange": "^10.0.0",
      "additionalData": {}
    },
    {
      "id": "gif-lib",
      "versionRange": "^5.2.2",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "kaleb",
      "versionRange": "^0.1.9",
      "additionalData": {
        "private": true
      }
    }
  ],
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "log": [
        "pwsh ./scripts/log.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "qpm qmod manifest",
        "pwsh ./scripts/createqmod.ps1 BSML -clean"
      ]
    }
  }
